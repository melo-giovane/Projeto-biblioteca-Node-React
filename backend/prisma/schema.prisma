
generator client {
  provider = "prisma-client-js"
  //output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id Int @id @default(autoincrement())
  email String @unique
  nome String
  senha String
  role UserRole @default(USUARIO)
}

enum UserRole {
  ADMIN
  FUNCIONARIO
  USUARIO
}